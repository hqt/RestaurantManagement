<h1>Listing temps</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @temps.each do |temp| %>
  <tr>
    <td><%= temp.username %></td>
    <td><%= temp.id %></td>
    <td><%= link_to 'Show', temp %></td>
    <td><%= link_to 'Edit', edit_temp_path(temp) %></td>
    <td><%= link_to 'Destroy', temp, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Temp', new_temp_path %>
